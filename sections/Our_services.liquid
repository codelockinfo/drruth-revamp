<div class="amr_our_services">
  <div class="amr_container_1170">
    <div class="amr_OS_flex">
      <div class="amr_OS_links">
        {% for block in section.blocks %}
          {% if block.type == 'link' %}
          {% endif %}
        {% endfor %}
      </div>
      <div class="amr_OS_img">
        {% for block in section.blocks %}
          {% if block.type == 'img' %}
          {% endif %}
        {% endfor %}
      </div>
      <div class="amr_OS_detail">
        {% for block in section.blocks %}
          {% if block.type == 'detail' %}
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
</div>
{% schema %}
{
  "name": "Our Services",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label":"Heading"
    }
  ],
  "blocks": [
    {
      "type": "img",
      "name": "Image",
      "limit":1,
      "settings": [
        {
          "type": "image_picker",
          "id": "main_img",
          "label":"Main Image"
        }
      ]
    },
    {
      "type": "link",
      "name": "link",
      "settings": [
        {
          "type": "text",
          "id": "btn_txt",
          "label":"Link Text"
        },
        {
          "type": "url",
          "id": "btn_url",
          "label":"Link URL"
        }
      ]
    },
    {
      "type": "detail",
      "name": "Detail",
      "limit":1,
      "settings": [
        {
          "type":"richtext",
          "id":"detail",
          "label":"Detail"
        },
        {
          "type": "text",
          "id": "btn_txtm",
          "label":"Button Text"
        },
        {
          "type": "url",
          "id": "btn_urlm",
          "label":"Button URL"
        }
      ]
    }
  ],
  "presets": [
      {
        "name": "Our Services"
      }
    ]
}
{% endschema %}