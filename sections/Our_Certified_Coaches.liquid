<div class="amr_certified_coaches">
  <div class="amr_container_1170">
    <div class="amr_pet_consultants_header">
      {% if section.settings.heading != blank %}<h2>{{ section.settings.heading }}</h2>{% endif %}
      {% if section.settings.desc != blank %}<div>{{ section.settings.desc }}</div>{% endif %}
    </div>
    <div class="amr_CC_items">
      {% for block in section.blocks %}
          <div class="amr_CC_item">
            
          </div>
      {% endfor %}
    </div>
  </div>
</div>
{% schema %}
{
  "name": "Our Certified Coaches",
  "max_blocks":8,
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label":"Heading"
    },
    {
      "type": "richtext",
      "id": "desc",
      "label":"Description"
    },
    {
      "type": "text",
      "id": "btn_txt_main",
      "label":"Button Text"
    },
    {
      "type": "url",
      "id": "btn_url_main",
      "label": "Button URL"
    }
  ],
  "blocks": [
    {
      "type": "consultants",
      "name": "Consultant",
      "settings": [
        {
          "type": "image_picker",
          "id": "bg_img",
          "label":"Background Image"
        },
        {
          "type": "text",
          "id": "title",
          "label":"Title"
        },
        {
          "type": "richtext",
          "id": "detail",
          "label":"Short Detail"
        },
        {
          "type": "text",
          "id": "btn_txt",
          "label":"Button Text"
        },
        {
          "type": "url",
          "id": "btn_url",
          "label": "Button URL"
        }
      ]
    }
  ],
  "presets": [
      {
        "name": "Our Certified Coaches"
      }
    ]
}
{% endschema %}