      "blocks": {
        "47588844-3245-4d6c-9603-2f57d9c9a97e": {
          "type": "points",
          "settings": {
            "title": "1. Apply",
            "detail": "<p>Fill out our application form and let us know why you’re a good fit for the program.<\/p>"
